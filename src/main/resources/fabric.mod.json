{
	"schemaVersion": 1,
	"id": "duration-enchant",
	"version": "${version}",
	"name": "Durational Enchant",
	"description": "Library Mod by zacharymei to provide Durational Enchantments to Minecraft. ",
	"authors": [
		"zacharymei"
	],
	"contact": {
		"homepage": "https://github.com/zacharymei",
		"sources": "https://github.com/zacharymei/duration-enchant"
	},
	"license": "CC0-1.0",
	"icon": "assets/duration-enchant/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"mods.zacharymei.de.DurationalEnchant",
			"mods.zacharymei.de.command.DECommands"
		],
		"client": [
			"mods.zacharymei.de.DurationalEnchantClient"
		],
		"fabric-datagen": [
			"mods.zacharymei.de.DurationalEnchantDataGenerator"
		]
	},
	"mixins": [
		"duration-enchant.mixins.json",
		{
			"config": "duration-enchant.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.4",
		"java": ">=17",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}